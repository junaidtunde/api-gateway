{
  "name": "micro-api-gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "audit-ci": "node ./node_modules/.bin/audit-ci --high",
    "clean": "trash dist coverage",
    "fl": "run-s fix lint",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier --write 'src/**/*.{ts,js,json}'",
    "fix:tslint": "tslint --project . --fix",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier -c 'src/**/*.{ts,js,json}'",
    "lint:tslint": "tslint --project .",
    "prebuild": "rimraf dist",
    "build": "run-s clean build:*",
    "build:main": "tsc --build",
    "build:static": "cpx './src/**/*.{html,png,jpg,mst,json,graphql}' dist",
    "build:google": "cpx './google-service-account.json' dist",
    "start": "nest start",
    "start:dev-01": "nest start --watch",
    "start:dev": "run-s clean build start:dev-01",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "chmod +x protoc.sh && npm run gen-proto",
    "gen-proto": "./protoc.sh"
  },
  "dependencies": {
    "@autochek-africa/proto": "4.5.16",
    "@autochek-africa/pubsub": "2.4.2",
    "@google-cloud/storage": "5.3.0",
    "@grpc/proto-loader": "0.5.5",
    "@hapi/joi": "17.1.1",
    "@nestjs/common": "7.6.14",
    "@nestjs/core": "7.4.4",
    "@nestjs/microservices": "7.4.4",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-express": "7.4.4",
    "@nestjs/serve-static": "2.1.3",
    "@nestjs/swagger": "4.6.0",
    "@nestjs/websockets": "7.5.5",
    "@types/passport-facebook": "2.1.10",
    "@types/passport-facebook-token": "0.4.34",
    "@types/socket.io-redis": "1.0.26",
    "@types/uuid": "8.3.0",
    "@vyng/truecaller-node": "1.1.0",
    "audit-ci": "3.1.1",
    "basic-auth": "2.0.1",
    "body-parser": "1.19.0",
    "camelcase-keys": "6.2.2",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "connect-redis": "6.0.0",
    "cookie-parser": "1.4.5",
    "cpx": "1.5.0",
    "dotenv": "8.2.0",
    "express-fileupload": "1.2.0",
    "express-rate-limit": "5.2.6",
    "express-session": "1.17.2",
    "grpc": "1.24.3",
    "grpc_tools_node_protoc_ts": "4.1.4",
    "grpc-tools": "1.9.1",
    "helmet": "4.1.0",
    "ip2location-nodejs": "8.3.0",
    "jimp": "0.16.1",
    "jsonwebtoken": "8.5.1",
    "npm-run-all": "4.1.5",
    "passport": "0.4.1",
    "passport-facebook": "3.0.0",
    "passport-facebook-token": "4.0.0",
    "passport-google-id-token": "0.4.7",
    "passport-google-oauth20": "2.0.0",
    "prom-client": "13.1.0",
    "publitio_js_sdk": "1.1.5",
    "puppeteer-core": "5.5.0",
    "rate-limit-redis": "2.1.0",
    "redis": "3.0.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.2",
    "socket.io-redis": "6.0.1",
    "swagger-ui-express": "4.1.4",
    "trash-cli": "3.1.0",
    "tslint": "6.1.3",
    "twing": "5.0.2",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.1",
    "@nestjs/schematics": "7.1.1",
    "@nestjs/testing": "7.4.4",
    "@types/express": "4.17.8",
    "@types/imagemin": "7.0.0",
    "@types/imagemin-jpegtran": "5.0.0",
    "@types/jest": "26.0.12",
    "@types/node": "14.6.2",
    "@types/passport-google-oauth20": "2.0.4",
    "@types/puppeteer": "5.4.2",
    "@types/puppeteer-core": "5.4.0",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "husky": "4.2.5",
    "jest": "26.4.2",
    "lint-staged": "10.2.13",
    "prettier": "2.1.1",
    "supertest": "4.0.2",
    "ts-jest": "26.3.0",
    "ts-loader": "8.0.3",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.0.2",
    "typescript-tslint-plugin": "0.5.5"
  },
  "private": true,
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "npm run lint:prettier",
    "!(*spec.).ts": "npm run lint:tslint",
    "*.{js,json,css}": "npm run lint:prettier"
  }
}
